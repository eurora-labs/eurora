{
	"productName": "Eurora",
	"identifier": "com.eurora.app",
	"mainBinaryName": "eurora",
	"build": {
		"beforeBuildCommand": "[ \"$CI\" = \"true\" ] || pnpm build:desktop -- --mode production"
	},
	"bundle": {
		"active": true,
		"resources": [
			"LICENSES-THIRD-PARTY.md",
			"docker-compose.yml",
			"hosts/mac.chromium.native-messaging.json",
			"hosts/mac.edge.native-messaging.json",
			"hosts/mac.firefox.native-messaging.json",
			"hosts/linux.chromium.native-messaging.json",
			"hosts/linux.edge.native-messaging.json",
			"hosts/linux.firefox.native-messaging.json"
		],
		"externalBin": [
			"binaries/euro-native-messaging"
		],
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"plugins": {
		"updater": {
			"endpoints": [
				"https://api.eurora-labs.com/releases/release/{{target}}-{{arch}}/{{current_version}}/{{bundle_type}}"
			],
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEU3NzQzMEU5RDE5MDk4OTMKUldTVG1KRFI2VEIwNTFCdlhJbkI5NExkOFpNVVpoQy9hZE1jQnlWR2FPZXYwR09rS1RLZ2dnc00K"
		}
	}
}
