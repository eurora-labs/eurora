{
  "productName": "Eurora Server",
  "identifier": "com.eurora-labs.eurora-server",
  "mainBinaryName": "euro-server",
  "bundle": {
    "active": true,
    "resources": [
      "config/authz/model.conf",
      "config/authz/policy.csv",
      "postgres/**"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": ["app", "dmg", "appimage", "deb", "rpm", "msi"],
    "macOS": {
      "minimumSystemVersion": "10.15"
    },
    "linux": {
      "deb": {
        "depends": ["libgtk-3-0", "libwebkit2gtk-4.1-0"]
      },
      "rpm": {
        "depends": ["webkit2gtk4.1"]
      }
    }
  },
  "app": {
    "windows": [],
    "withGlobalTauri": false
  }
}
